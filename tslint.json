{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ false ],
      "no-shadowed-variable": [ false ],
      "no-console": false,
      "no-bitwise": false,
      "max-classes-per-file": false,
      "no-empty": false,
      "prefer-const": false,
      "prefer-for-of": false
  },
  "rulesDirectory": []
}